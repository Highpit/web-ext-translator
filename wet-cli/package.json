{
  "name": "web-ext-translator",
  "version": "3.0.0",
  "description": "An easy to use translation tool for web-extensions",
  "keywords": [
    "Web-Extensions",
    "Translation",
    "Translator"
  ],
  "homepage": "https://github.com/Lusito/web-ext-translator",
  "bugs": {
    "url": "https://github.com/Lusito/web-ext-translator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lusito/web-ext-translator.git"
  },
  "license": "Zlib",
  "author": "Santo Pfingsten",
  "main": "dist/main.js",
  "bin": {
    "wet": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc && npm run copy",
    "clean": "rimraf dist && rimraf README.md",
    "copy": "cpx ../docs/**/*.* ./dist/docs && cpx ../README.md .",
    "lint:package": "sort-package-json --check",
    "lint:package:fix": "sort-package-json",
    "start": "electron ."
  },
  "dependencies": {
    "electron": "^8.2.0",
    "file-url": "^3.0.0",
    "web-ext-translator-shared": "^3.0.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.40.0",
    "typescript": "^3.8.3"
  },
  "preferGlobal": true
}
